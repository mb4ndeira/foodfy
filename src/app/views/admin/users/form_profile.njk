<section class="form wrapper users">
    <div class="field_container">
        <h3>Nome</h3>
        <input type="text" placeholder="Digite o nome Completo" name="name" class="field" value="{{items.name}}">
    </div>
    <div class="field_container">
        <h3>Email</h3>
        <input type="email" placeholder="Digite o email" name="email" class="field" value="{{items.email}}">
    </div>
    <div class="field_container">
        <h3>Senha</h3>
        <input type="password" placeholder="Digite a senha" name="password" class="field">
    </div>
   
</section>